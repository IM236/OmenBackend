{
  "name": "market-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch --tsconfig tsconfig.dev.json src/server.ts",
    "start": "node dist/server.js",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run src/tests/integration",
    "migrate": "tsx --tsconfig tsconfig.dev.json src/infra/database/migrations/runMigrations.ts"
  },
  "engines": {
    "node": ">=18.18"
  },
  "dependencies": {
    "bullmq": "^5.7.8",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pg": "^8.11.5",
    "pino": "^9.1.0",
    "pino-http": "^9.0.0",
    "undici": "^6.19.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "pino-pretty": "^11.2.2",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
