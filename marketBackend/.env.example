NODE_ENV=development
PORT=3000
LOG_LEVEL=debug
REQUEST_TIMEOUT_MS=15000

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/market_backend
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10
DATABASE_SSL=false

REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_TLS=false

ENTITY_PERMISSIONS_BASE_URL=http://localhost:8000/api/v1
ENTITY_PERMISSIONS_API_KEY=local-dev-token
ENTITY_PERMISSIONS_TIMEOUT_MS=5000

SAPPHIRE_RPC_URL=https://testnet.sapphire.oasis.dev
SAPPHIRE_CHAIN_ID=0x5aff
SAPPHIRE_MAX_FEE_CEILING=100000000
SAPPHIRE_RATE_LIMIT_PER_MINUTE=120
OASIS_WALLET_MNEMONIC="replace with mnemonic"
CONFIDENTIAL_SIGNER_PRIVATE_KEY=

TRANSACTION_QUEUE_NAME=market-tx-queue
DLQ_QUEUE_NAME=market-tx-dlq
MAX_RETRY_ATTEMPTS=5
RETRY_BACKOFF_MS=2000

ADMIN_API_KEY=local-admin-key
ADMIN_JWT_PUBLIC_KEY=

RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

ENABLE_WEBSOCKETS=true
